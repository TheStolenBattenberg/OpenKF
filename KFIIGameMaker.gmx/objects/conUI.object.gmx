            <kind>1</kind>
            <string>///Draw UIs to surface

//Make sure surface exists.
if(!surface_exists(surface))
    surface = surface_create(conMain.GameConfig[? "renderWidth"], conMain.GameConfig[? "renderHeight"]);


//Set up surface for rendering UI
surface_set_target(surface);
draw_clear_alpha(c_black, 0.0);

var uiID = ds_list_size(uiList);
do {
    //Make sure uiID it's not 0 when we run this loop
    if(uiID &lt;= 0)
        break;
    
    //Grab UI object
    var uiObject = uiList[| uiID-1];
    
    //Draw UI
    with(uiObject)
        event_perform(ev_other, ev_user2);
     
    uiID--;   
} until(uiObject.stopDraw == true);

surface_reset_target();
</string>
